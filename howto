### Preparing dSFMT
cd dSFMT-src-2.2.1
gcc -DDSFMT_DO_NOT_USE_OLD_NAMES -DDSFMT_MEXP=216091 -msse2 -DHAVE_SSE2 \
-c dSFMT.c
cd ..

# Once updating dSFMT, copy dSFMT.h into include/


### Compile pw
gcc -c -Iinclude seeds.c
gcc -c -DDSFMT_MEXP=216091 -Iinclude pw.c
gcc -o pw seeds.o dSFMT-src-2.2.1/dSFMT.o pw.o
